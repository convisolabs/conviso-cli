        {
          "ruleId": "SOURCE_CODE_VULNERABILITY",
          "level": "warning",
          "message": {
            "text": "Credenciais Expostas no C\u00f3digo"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "OWASPWebGoatPHP (github.com/OWASP)"
                },
                "region": {
                  "startLine": 1
